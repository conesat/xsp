<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hg.xsp.dao.SelectMapper">
	<select id="selectListNo" resultType="com.hg.xsp.entity.Student">
		select * from student where state="未提交";
	</select>
	<select id="selectListYes" resultType="com.hg.xsp.entity.Student">
		select * from student where state="已提交";
	</select>
	<select id="selectListMsg" resultType="String">
		select msg from msg order by id desc;
	</select>
</mapper>